<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ecom.javacodings.customer.access.TagDAO">

<select id="getTag" resultType="tagDTO" parameterType="String">
    select *
    from TAGS
    where TAG_ID = #{value}
</select>

</mapper>
 