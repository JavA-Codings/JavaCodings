<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "HTTP://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="cacheEnabled" value="false" />
        <setting name="useGeneratedKeys" value="true" />
        <setting name="defaultExecutorType" value="REUSE" />
    </settings>

    <!-- aliases - short name setting bvo, pvo-->
    <typeAliases>
        <typeAlias alias="hashMap" type="java.util.HashMap" />
        <typeAlias alias="map" type="java.util.Map" />

        <typeAlias alias="MemberDTO" type="com.ecom.javacodings.common.transfer.MemberDTO" />
        <typeAlias alias="AddressDTO" type="com.ecom.javacodings.common.transfer.MemberAddressDTO" />
        <typeAlias alias="PaymentDTO" type="com.ecom.javacodings.common.transfer.MemberPaymentDTO" />

        <typeAlias alias="itemDTO" type="com.ecom.javacodings.common.transfer.ItemDTO" />
        <typeAlias alias="ImageDTO" type="com.ecom.javacodings.common.transfer.ItemImageDTO" />

        <typeAlias alias="bannerDTO" type="com.ecom.javacodings.common.transfer.BannerDTO" />
        <typeAlias alias="orderDTO" type="com.ecom.javacodings.common.transfer.OrderDTO" />

        <typeAlias alias="cartDTO" type="com.ecom.javacodings.common.transfer.CartDTO" />
        <typeAlias alias="eventDTO" type="com.ecom.javacodings.common.transfer.table.EventDTO" />

        <typeAlias alias="pageDTO" type="com.ecom.javacodings.common.page.PageDTO" />
        <typeAlias alias="summaryDTO" type="com.ecom.javacodings.common.transfer.SummaryDTO" />
    </typeAliases>
</configuration>